.pge-vipList-wrapper{    
    width: 100vw;
    padding-top: 88rpx;
    background-color: #F5F5F5;
}
.pge-tab-common{
	z-index: 100;
}
.pge-listNone-info{
  text-align: center;
  margin-top: 100rpx;
}
.pge-listNone-info image{
    width: 100%;
    height: auto;
}
.pge-vipList-wrapper .pge-error-msg{
  color: #333333;
  margin-top: 20rpx;
}

.pge-tab-common{
	border-top:1rpx solid #f5f5f5;
	width: 100%;
	height: 88rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
	color: #333333;
	position: absolute;
	top: 0;
	left: 0;
	border-bottom: 1px solid #F9F8FF;
	background-color: #fff;
}
.pge-tab-item{
    width: 50%;
    text-align: center;
    font-size: 28rpx;
}
.pge-tab-item text{
    line-height: 80rpx;
}
.pge-tab-item.pge-active{
    font-size: 32rpx;
    color: #1677FF;

}
.pge-tab-item.active .pge-border-active{
    width: 55rpx;
    height: 6rpx;
    background-color: #2088FF;
    border-radius: 3rpx;
    margin: 0 auto;
}