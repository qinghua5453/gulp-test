.tpl-head{
    height: 104rpx;
    width: 100%;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 34rpx 0 24rpx;
    margin: 24rpx auto;
    font-size: 34rpx;
}
.tpl-head-extra{
    color: #999999;  
    font-size: 30rpx;  
}
.tpl-head-extra .tpl-active{
	color: #333333;
}
.tpl-feedback-detail{
    width: 710rpx;
    margin: 0 auto;
    padding: 40rpx 20rpx 0;
    background-color: #fff;
    border-radius:20rpx;
}
.tpl-feedback-radio-list{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background-color: #f5f5f5;
    padding-top: 20rpx;
	z-index: 100;
}
.tpl-feedback-radio-list .tpl-list{
    height: 103rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 30rpx;
    border-bottom: 2rpx solid #f9f8ff;
    color: #333333;
}
.tpl-feedback-radio-list .tpl-list-title{
    font-size: 32rpx;
}
.tpl-feedback-radio-list .iconfont{
    font-size: 44rpx;
    color: #CCCCCC;
}
.tpl-feedback-radio-list .icon-xuanzhongx{
    color:#2088FF;
}