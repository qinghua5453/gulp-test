@import '/common/iconfont/iconfont.wxss'; 
@import '/common/reset.wxss'; 
@import '/common/btn-common.wxss';
.cmp-list{
    width: 100%;
    height: 138rpx;
    border-bottom: 2rpx solid #F9F8FF;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.cmp-list:last-child{
    border-bottom: none;
}
.cmp-list .cmp-name{
    display: block;
    font-size: 32rpx;
    line-height: 45rpx;
    margin-bottom: 8rpx;
    color: #333333;
}
.cmp-list .cmp-time{
    line-height: 40rpx;
    color: #999999;
    font-size: 28rpx;
}
.cmp-list .cmp-price{
    font-size: 36rpx;
    color: #333333;
}
.cmp-list.cmp-add .cmp-name{
    color: #2088FF;
}
.cmp-list.add .cmp-price{
    color: #2088FF;
}