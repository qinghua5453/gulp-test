.cmp-coupon-wrapper{
  width: 100%;
  height: 100%;
  padding-bottom: 130rpx;
  position: fixed;
  box-sizing: border-box;
  z-index: 99;
  background: #f5f5f5;
  overflow: auto;
  top: 0;
}
.cmp-coupon-list{
    padding: 0 30rpx;
}
.cmp-no-list{
    position: absolute;
    top: 386rpx;
    text-align: center;
    width: 100%;   
}
.cmp-no-list image{
    width: 170rpx;
    height: auto;
    margin: 0 auto 67rpx;
    display: block;
}
.cmp-no-list text{
    color: #8a8fab;
    font-size: 28rpx;
}
.cmp-close{
	position: fixed;
	width: 100%;
	line-height:100rpx;
	text-align: center;
	background-color: #fff;
	font-size:  36rpx;
	bottom: 0;
	left: 0;
}
